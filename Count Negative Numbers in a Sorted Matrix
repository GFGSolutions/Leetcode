// User template for Java

class Solution {
  public int countNegatives(int[][] grid) {
    int m = grid.length;
    int n = grid[0].length;
    int countNegs = 0;
    for(int[] array : grid) {
      int pos_s = 0;
      for(int i = 0; i < array.length; i++) {
        if(array[i] >= 0) {
          pos_s++;
        }
      }
      int neg_s = n - pos_s;
      countNegs += neg_s;
    }
    return countNegs;
  }
}
