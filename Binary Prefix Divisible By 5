// User template for Java

import java.util.*;
class Solution {
  public List<Boolean> prefixesDivBy5(int[] nums) {
    List<Boolean> ans = new ArrayList<>(nums.length);
    int rem = 0;
    for (int bit : nums) {
      rem = (rem * 2 + bit) % 5;
      ans.add(rem == 0);
    }
    return ans;
  }
}
